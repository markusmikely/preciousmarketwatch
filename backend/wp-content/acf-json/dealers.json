{
  "key": "group_dealers",
  "title": "Dealers",
  "fields": [
    {
      "key": "field_logo",
      "label": "Dealer Logo",
      "name": "logo",
      "type": "image",
      "return_format": "url"
    },
    {
      "key": "field_rating",
      "label": "Rating",
      "name": "rating",
      "type": "number",
      "min": 0,
      "max": 5,
      "step": 0.1
    },
    {
      "key": "field_review_link",
      "label": "Review Link",
      "name": "review_link",
      "type": "url"
    },
    {
      "key": "field_affiliate_link",
      "label": "Affiliate Link",
      "name": "affiliate_link",
      "type": "url"
    },
    {
      "key": "field_featured",
      "label": "Featured Dealer",
      "name": "featured",
      "type": "true_false"
    },
    {
      "key": "field_verified",
      "label": "Verified Dealer",
      "name": "is_verified",
      "type": "true_false"
    },
    {
      "key": "field_short_description",
      "label": "Short Description",
      "name": "short_description",
      "type": "textarea"
    },
    {
      "key": "field_sub_categories",
      "label": "Sub Categories",
      "name": "sub_categories",
      "type": "text"
    },
    {
      "key": "field_metals",
      "label": "Metals Sold",
      "name": "metals",
      "type": "checkbox",
      "choices": {
        "gold": "Gold",
        "silver": "Silver",
        "platinum": "Platinum",
        "palladium": "Palladium"
      }
    },
    {
      "key": "field_gemstones",
      "label": "Gemstones Sold",
      "name": "gemstones",
      "type": "checkbox",
      "choices": {
        "diamond": "Diamond",
        "ruby": "Ruby",
        "sapphire": "Sapphire",
        "emerald": "Emerald"
      }
    }
  ],
  "location": [
    [
      {
        "param": "post_type",
        "operator": "==",
        "value": "dealer"
      }
    ]
  ]
}
